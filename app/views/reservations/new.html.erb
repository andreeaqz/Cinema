<h1>Reservations#new <%= @film_id %></h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
	<%= form_for(@reservation, url: selectseats_path) do |f| %>
		<%= f.label :running_film_id %>
		<%= f.collection_select(:running_film_id, @rf, :id, :date_time) %>

		<%= f.submit %>
	<% end %>
  </div>
</div>